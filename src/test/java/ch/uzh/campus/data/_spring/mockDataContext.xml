<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
  http://www.springframework.org/schema/util 
  http://www.springframework.org/schema/util/spring-util-2.0.xsd">


    <!-- ======================================================================================= -->
    <!-- ===============DataGenerator================= -->
    <!-- ======================================================================================= -->
    <bean id="mockDataGenerator" class="ch.uzh.campus.data.MockDataGenerator" scope="prototype" >
        <property name="orgs" ref="orgs" />
        <property name="sapOlatUsers" ref="sapOlatUsers" />
        <property name="delegations" ref="delegations" />
        <property name="students" ref="students" />
        <property name="lecturers" ref="lecturers" />
        <property name="courses" ref="courses" />
        <property name="textCourseIds" ref="textCourseIds" />
        <property name="eventCourseIds" ref="eventCourseIds" />
        <property name="importStatistics" ref="importStatistics" />
        <property name="studentIdCourseIds" ref="studentIdCourseIds" />
        <property name="lecturerIdCourseIds" ref="lecturerIdCourseIds"/>
    </bean>

    <!-- ======================================================================================= -->
    <!-- ===============Student================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'students' scope="prototype" >
        <bean class="ch.uzh.campus.data.Student" p:id="2100" p:registrationNr="1000" p:firstName="firstName1" p:lastName="lastName1" p:email="email1" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.Student" p:id="2200" p:registrationNr="2000" p:firstName="firstName2" p:lastName="lastName2" p:email="email2" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.Student" p:id="2300" p:registrationNr="3000" p:firstName="firstName3" p:lastName="lastName3" p:email="email3" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============Lecturer================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'lecturers' scope="prototype" >
        <bean class="ch.uzh.campus.data.Lecturer" p:personalNr="1100" p:firstName="firstName1" p:lastName="lastName1" p:email="email1" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.Lecturer" p:personalNr="1200" p:firstName="firstName2" p:lastName="lastName2" p:email="email2" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============Course================= -->
    <!-- ======================================================================================= -->
    <bean id="course_1" class="ch.uzh.campus.data.Course" p:id="100" p:resourceableId="100" p:shortTitle="shortTitle1" p:title="title1" p:vstNr="vstNr1" p:category="Seminar" p:language="EN" p:vvzLink="link" p:semester="hs" p:shortSemester="HS" p:enabled="1" p:synchronizable="true" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    <bean id="course_2" class="ch.uzh.campus.data.Course" p:id="200" p:resourceableId="200" p:shortTitle="shortTitle2" p:title="title2" p:vstNr="vstNr2" p:category="Seminar" p:language="EN" p:vvzLink="link" p:semester="hs" p:shortSemester="HS" p:enabled="1" p:synchronizable="true" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    <bean id="course_3" class="ch.uzh.campus.data.Course" p:id="300" p:shortTitle="shortTitle3" p:title="title3" p:vstNr="vstNr3" p:category="Seminar" p:language="EN" p:vvzLink="link" p:semester="hs" p:shortSemester="HS" p:enabled="1" p:synchronizable="true" p:modifiedDate="10.10.2014 10:10" scope="prototype">
        <property name="resourceableId"><null/></property>
    </bean>
    <util:list id = 'courses' scope="prototype" >
        <ref bean="course_1"/>
        <ref bean="course_2"/>
        <ref bean="course_3"/>
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============StudentIdCourseIds================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'studentIdCourseIds' scope="prototype" >
        <bean class="ch.uzh.campus.data.StudentIdCourseId" p:studentId="2100" p:courseId="100" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.StudentIdCourseId" p:studentId="2200" p:courseId="100" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.StudentIdCourseId" p:studentId="2100" p:courseId="200" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.StudentIdCourseId" p:studentId="2200" p:courseId="200" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.StudentIdCourseId" p:studentId="2100" p:courseId="300" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>
      

    <!-- ======================================================================================= -->
    <!-- ===============LecturerIdCourseIds================= -->
    <!-- ======================================================================================= -->
    <util:list id="lecturerIdCourseIds" scope="prototype" >
        <bean id="lecturerIdCourseId1" class="ch.uzh.campus.data.LecturerIdCourseId" p:courseId="100" p:lecturerId="1100" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean id="lecturerIdCourseId2" class="ch.uzh.campus.data.LecturerIdCourseId" p:courseId="200" p:lecturerId="1100" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean id="lecturerIdCourseId3" class="ch.uzh.campus.data.LecturerIdCourseId" p:courseId="200" p:lecturerId="1200" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean id="lecturerIdCourseId4" class="ch.uzh.campus.data.LecturerIdCourseId" p:courseId="300" p:lecturerId="1100" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============TextCourseIds================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'textCourseIds' scope="prototype" >
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Veranstaltungsinhalt" p:lineSeq="1" p:line="- praktische Tätigkeiten im chemischen Labor" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Veranstaltungsinhalt" p:lineSeq="2" p:line="- Herstellung von Lösungen unterschiedlicher Konzentration" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Unterrichtsmaterialien" p:lineSeq="1" p:line="Versuchsanleitungen," p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Unterrichtsmaterialien" p:lineSeq="2" p:line="download von homepage (s. link)" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Hinweise" p:lineSeq="1" p:line="Selbsttestfragen:" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="100" p:type="Hinweise" p:lineSeq="2" p:line="Zugriff über www.vetpharm.uzh.ch/cyberpharm" p:modifiedDate="10.10.2014 10:10" scope="prototype" />

        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="200" p:type="Veranstaltungsinhalt" p:lineSeq="1" p:line="- praktische Tätigkeiten im chemischen Labor2" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.TextCourseId" p:courseId="200" p:type="Veranstaltungsinhalt" p:lineSeq="2" p:line="- Herstellung von Lösungen unterschiedlicher Konzentration2" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============Org================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'orgs' scope="prototype" >
        <bean class="ch.uzh.campus.data.Org" p:id="100" p:shortName="1" p:name="Theologische Fakultät" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.Org" p:id="200" p:shortName="3" p:name="Wirtschaftswissenschaftliche Fakultät" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============EventCourseIds================= -->
    <!-- ======================================================================================= -->
   <util:list id = 'eventCourseIds' scope="prototype" >
        <bean class="ch.uzh.campus.data.EventCourseId" p:courseId="100" p:date="13.10.2014 00:00:00" p:start="10:00:00" p:end="11:30:00" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.EventCourseId" p:courseId="100" p:date="13.10.2014 00:00:00" p:start="16:00:00" p:end="17:30:00" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.EventCourseId" p:courseId="200" p:date="16.10.2014 00:00:00" p:start="09:00:00" p:end="10:30:00" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
        <bean class="ch.uzh.campus.data.EventCourseId" p:courseId="200" p:date="16.10.2014 00:00:00" p:start="14:00:00" p:end="15:30:00" p:modifiedDate="10.10.2014 10:10" scope="prototype" />
    </util:list>
	
    <!-- ======================================================================================= -->
    <!-- ===============SapOlatUser================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'sapOlatUsers' scope="prototype" >
        <bean class="ch.uzh.campus.data.SapOlatUser" p:sapUserId="100" p:olatUserName="olatUserName1" p:kindOfMapping="AUTO" p:sapUserType="STUDENT" p:mappingTimeStamp-ref="newDate" scope="prototype" />
        <bean class="ch.uzh.campus.data.SapOlatUser" p:sapUserId="200" p:olatUserName="olatUserName2" p:kindOfMapping="AUTO" p:sapUserType="LECTURER" p:mappingTimeStamp-ref="newDate" scope="prototype" />
    </util:list>


    <!-- ======================================================================================= -->
    <!-- ===============Delegation================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'delegations' scope="prototype" >
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator1" p:delegatee="delegatee11" scope="prototype" />
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator1" p:delegatee="delegatee12" scope="prototype" />
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator2" p:delegatee="delegatee20" scope="prototype" />
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator3" p:delegatee="delegatee20" scope="prototype" />
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator4" p:delegatee="delegatee40" scope="prototype" />
        <bean class="ch.uzh.campus.data.Delegation" p:delegator="delegator5" p:delegatee="delegatee50" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============ImportStatistic================= -->
    <!-- ======================================================================================= -->
    <util:list id = 'importStatistics' scope="prototype" >
        <bean class="ch.uzh.campus.data.ImportStatistic" p:stepId="10" p:stepName="import_controlFile" p:status="COMPLETED" p:startTime="10.10.2014 10:10" p:endTime="10.10.2014 10:12" p:readCount="8" p:writeCount="8" p:readSkipCount="0" p:writeSkipCount="0" p:processSkipCount="0" p:commitCount="0" p:rollbackCount="0" scope="prototype" />
        <bean class="ch.uzh.campus.data.ImportStatistic" p:stepId="20" p:stepName="import_controlFile" p:status="COMPLETED" p:startTime="11.10.2014 10:10" p:endTime="11.10.2014 10:12" p:readCount="8" p:writeCount="8" p:readSkipCount="0" p:writeSkipCount="0" p:processSkipCount="0" p:commitCount="0" p:rollbackCount="0" scope="prototype" />
        <bean class="ch.uzh.campus.data.ImportStatistic" p:stepId="30" p:stepName="import_controlFile" p:status="FAILED" p:startTime="12.10.2014 10:10" p:endTime="12.10.2014 10:12" p:readCount="8" p:writeCount="7" p:readSkipCount="0" p:writeSkipCount="1" p:processSkipCount="0" p:commitCount="0" p:rollbackCount="0" scope="prototype" />
    </util:list>

    <!-- ======================================================================================= -->
    <!-- ===============Date================= -->
    <!-- ======================================================================================= -->
    <bean id="newDate" class="java.util.Date" scope="prototype" />

    <!-- ======================================================================================= -->
    <!-- ===============Custom Editors================= -->
    <!-- ======================================================================================= -->
    <bean id="customEditorConfigurer"
          class="org.springframework.beans.factory.config.CustomEditorConfigurer">
        <property name="customEditors">
            <map>
                <entry key="java.util.Date">
                    <bean class="org.springframework.beans.propertyeditors.CustomDateEditor">
                        <constructor-arg index="0">
                            <bean class="java.text.SimpleDateFormat">
                                <constructor-arg value="dd.MM.yyyy HH:mm" />
                            </bean>
                        </constructor-arg>
                        <constructor-arg index="1" value="false" />
                    </bean>
                </entry>
            </map>
        </property>
    </bean>

</beans>
